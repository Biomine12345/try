  <link href="/libraries/porto/HTML/css/demos/demo-restaurant.css" rel="stylesheet">
  {#<link href="/assets/css/pages/index.css" rel="stylesheet">#}
{% endblock %}

{% block page_body_class %}home{% endblock %}

{% block page_content %}
