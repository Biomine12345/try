{% extends "/global_page_template" ~ (IS_DEVEL_SERVER ? "_dev") ~ ".twig" %}

{% block page_title %}
  {% if constant('HTML_LANG') is sameas('zh-Hans') %}
    Leading you to a healthier life!
  {% elseif constant('HTML_LANG') is sameas('zh-Hant') %}
    Leading you to a healthier life!
  {% else %}
    Leading you to a healthier life!
  {% endif %}
{% endblock %}

{% block page_desc %}
  {% if constant('HTML_LANG') is sameas('zh-Hans') %}
  {% elseif constant('HTML_LANG') is sameas('zh-Hant') %}
  {% else %}
  {% endif %}
{% endblock %}

{% block page_plugin_styles %}
  {{ parent() }}
  <link href="//cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.0/assets/owl.carousel.min.css" rel="stylesheet"/>
